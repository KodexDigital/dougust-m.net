	<script type="text/javascript">
				jQuery(document).ready(function ($) {
					$(".scroll").click(function (event) {
						event.preventDefault();
						$('html,body').animate({ scrollTop: $(this.hash).offset().top }, 900);
					});
				});
	</script>
